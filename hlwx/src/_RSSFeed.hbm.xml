<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.acmeamalgamatedwidgets.com.RSSFeed" table="RSSFeed">
   <id name="idRSSFeed" type="long" column="idRSSFeed">
   <generator class="assigned"/>
  </id>

  <property name="RSSFeedName">
   <column name="RSSFeedName"/>
  </property>
  <property name="RSSFeedURL">
  <column name="RSSFeedURL"/>
  </property>
  <property name="RSSFeedInterval">
  <column name="RSSFeedInterval"/>
  </property>
 </class>
</hibernate-mapping>